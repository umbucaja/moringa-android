<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="umbucaja.moringa.fragments.DesenvolvedoresFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:orientation="horizontal"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:id="@+id/linearLayout2"
        android:layout_above="@+id/linearLayout3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="33">

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:src="@drawable/andre"
                app:civ_border_color="#FFFFFF"
                app:civ_border_width="1dp"
                android:id="@+id/view"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Andre"
                android:id="@+id/textView1"
                android:layout_below="@+id/view"
                android:layout_centerHorizontal="true" />

            <ImageButton
                android:layout_width="37dp"
                android:layout_height="37dp"
                android:src="@mipmap/ic_linkedin"
                android:id="@+id/ib_linkedin_andre"
                android:onClick="openNavigation"
                android:tag="https://br.linkedin.com/in/andrefnmuniz"
                android:layout_below="@+id/textView1"
                android:layout_toLeftOf="@+id/space"
                android:layout_toStartOf="@+id/space" />
            <ImageButton
                android:layout_width="37dp"
                android:layout_height="37dp"
                android:src="@mipmap/ic_github"
                android:onClick="openNavigation"
                android:tag="https://github.com/Netherdan"
                android:id="@+id/imageButton2"
                android:layout_below="@+id/textView1"
                android:layout_toRightOf="@+id/space"
                android:layout_toEndOf="@+id/space" />

            <Space
                android:layout_width="5dp"
                android:layout_height="20px"
                android:layout_below="@+id/textView1"
                android:layout_centerHorizontal="true"
                android:layout_alignBottom="@+id/ib_linkedin_andre"
                android:id="@+id/space" />
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="33">

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:src="@drawable/jordao"
                app:civ_border_color="#FFFFFF"
                app:civ_border_width="1dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:id="@+id/view2" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Jordão Serafim"
                android:id="@+id/textView2"
                android:layout_below="@+id/view2"
                android:layout_centerHorizontal="true" />

            <ImageButton
                android:layout_width="37dp"
                android:layout_height="37dp"
                android:src="@mipmap/ic_linkedin"
                android:id="@+id/ib_linkedin_jordao"
                android:onClick="openNavigation"
                android:tag="https://br.linkedin.com/in/jordao-serafim-48516723"
                android:layout_below="@+id/textView2"
                android:layout_toLeftOf="@+id/space2"
                android:layout_toStartOf="@+id/space2" />
            <ImageButton
                android:layout_width="37dp"
                android:layout_height="37dp"
                android:src="@mipmap/ic_github"
                android:onClick="openNavigation"
                android:tag="https://github.com/jordaoesa"
                android:id="@+id/imageButton"
                android:layout_below="@+id/textView2"
                android:layout_toRightOf="@+id/space2"
                android:layout_toEndOf="@+id/space2" />

            <Space
                android:layout_width="5dp"
                android:layout_height="20px"
                android:id="@+id/space2"
                android:layout_below="@+id/textView2"
                android:layout_centerHorizontal="true"
                android:layout_alignBottom="@+id/ib_linkedin_jordao" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="33">

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:src="@drawable/luiz"
                app:civ_border_color="#FFFFFF"
                app:civ_border_width="1dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:id="@+id/view3" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Luiz Corrêa"
                android:id="@+id/textView3"
                android:layout_below="@+id/view3"
                android:layout_centerHorizontal="true" />

            <ImageButton
                android:layout_width="37dp"
