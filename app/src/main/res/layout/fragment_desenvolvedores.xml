                android:layout_alignStart="@+id/view5" />
            <ImageButton
                android:layout_width="37dp"
                android:layout_height="37dp"
                android:src="@drawable/ic_github"
                android:onClick="openNavigation"
                android:tag="https://github.com/romeryto"
                android:layout_below="@+id/textView5"
                android:layout_toRightOf="@+id/ib_linkedin_romeryto"
                android:layout_toEndOf="@+id/ib_linkedin_romeryto" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="33">

            <de.hdodenhof.circleimageview.CircleImageView
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:src="@drawable/thiago"
                app:civ_border_color="#FFFFFF"
                app:civ_border_width="1dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:id="@+id/view6" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Thiago Medeiros"
                android:id="@+id/textView6"
                android:layout_below="@+id/view6"
                android:layout_centerHorizontal="true" />

            <ImageButton
                android:layout_width="37dp"
                android:layout_height="37dp"
                android:src="@drawable/ic_linkedin"
                android:id="@+id/ib_linkedin_thiago"
                android:onClick="openNavigation"
                android:tag="https://br.linkedin.com/in/thiago-almeida-144430a7"
                android:layout_below="@+id/textView6"
                android:layout_alignLeft="@+id/view6"
                android:layout_alignStart="@+id/view6" />
            <ImageButton
                android:layout_width="37dp"
                android:layout_height="37dp"
                android:src="@drawable/ic_github"
                android:onClick="openNavigation"
                android:tag="https://github.com/thiagoalmedeiros"
                android:layout_below="@+id/textView6"
                android:layout_toRightOf="@+id/ib_linkedin_thiago"
