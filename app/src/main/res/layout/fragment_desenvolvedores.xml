            <ImageButton
                android:layout_width="37dp"
                android:layout_height="37dp"
                android:src="@mipmap/ic_linkedin"
                android:id="@+id/ib_linkedin_thiago"
                android:onClick="openNavigation"
                android:tag="https://br.linkedin.com/in/thiago-almeida-144430a7"
                android:layout_below="@+id/textView6"
                android:layout_alignLeft="@+id/view6"
