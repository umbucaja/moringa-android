                android:src="@mipmap/ic_linkedin"
                android:id="@+id/ib_linkedin_luiz"
                android:onClick="openNavigation"
                android:tag="https://www.linkedin.com/in/luizantonioccf"
                android:layout_below="@+id/textView3"
                android:layout_toLeftOf="@+id/space3"
                android:layout_toStartOf="@+id/space3" />
            <ImageButton
                android:layout_width="37dp"
                android:layout_height="37dp"
                android:src="@mipmap/ic_github"
                android:onClick="openNavigation"
                android:tag="https://github.com/laccf"
                android:layout_below="@+id/textView3"
                android:layout_toRightOf="@+id/space3"
                android:layout_toEndOf="@+id/space3" />

            <Space
                android:layout_width="5dp"
                android:layout_height="20px"
                android:id="@+id/space3"
                android:layout_alignBottom="@+id/ib_linkedin_luiz"
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/textView3" />
        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
